</ul>
<div style="margin-left:11%;padding:1px 16px;height:auto;">
    <div class="table-responsive">
        <table class="table table-condensed">
            <thead>
            <tr>
                <th>Product Name</th>
                <th>Category</th>
                <th>Price</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            {{#each data}}
                <tr>
                    <td>{{this.productName}}</td>
                    <td>{{this.category}}</td>
                    <td><b>$</b>{{this.price}}</td>
                    <td>
                        <a class="btn btn-primary" href="/products/view/{{this.id}}">View</a>
                    </td>
                </tr>
            {{/each}}
            </tbody>
            <tfooter>
            </tfooter>
        </table>
    </div>
</div>
