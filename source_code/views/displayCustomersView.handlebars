<ul>
    <li><a href="/admin/{{user}}/clients">List of Clients</a></li>
    <li><a href="/admin/{{user}}/products">List of Products</a></li>
    <li><a href="/admin/{{user}}/addProduct">Add a Product</a></li>
    <li><a href="/admin/{{user}}/orders">List of Orders</a></li>
    <li><a href="/admin/{{user}}/search">Search</a></li>
</ul>
<div style="margin-left:11%;padding:1px 16px;height:auto;">
    <div class="table-responsive">
        <table class="table table-condensed">
            <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Phone</th>
                <th>Email</th>
                <th>City</th>
                <th>State</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            {{#each data}}
                <tr>
                    <td>{{this.userFirstname}}</td>
                    <td>{{this.userLastname}}</td>
                    <td>{{this.userPhone}}</td>
                    <td>{{this.userEmail}}</td>
                    <td>{{this.userCity}}</td>
                    <td>{{this.userState}}</td>
                    <td>
                        <a class="btn btn-primary" href="/admin/{{this.user}}/clients/view/{{this.id}}">View</a>
                        <a class="btn btn-primary" href="/admin/{{this.user}}/clients/edit/{{this.id}}">Edit</a>
                        <a class="btn btn-primary" href="/admin/{{this.user}}/clients/delete/{{this.id}}">Delete</a>
                    </td>
                </tr>
            {{/each}}
            </tbody>
            <tfooter>
            </tfooter>
        </table>
    </div>
</div>
